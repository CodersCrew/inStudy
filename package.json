{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon --exec \"yarn run parse-server && node dist/index.js\"",
    "parse-server": "rimraf dist/ && babel ./server/ --out-dir dist/ --ignore ./node_modules",
    "client": "cd client && yarn run start",
    "build": "yarn run parse-server && cd client && yarn run build",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "install:all": "yarn install && cd client && yarn install",
    "lint": "yarn run eslint --ext .jsx --ext .js client/",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false YARN_PRODUCTION=false yarn run install:all && yarn run parse-server && yarn run build --prefix client"
  },
  "author": "Konrad Szwarc",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.56",
    "@babel/core": "7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.56",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.56",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.56",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.56",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.56",
    "@babel/preset-env": "7.0.0-beta.56",
    "@babel/preset-react": "7.0.0-beta.56",
    "@babel/preset-stage-0": "7.0.0-beta.56",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.5.0",
    "stylelint-processor-styled-components": "^1.3.2",
    "supports-color": "^5.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.56",
    "base64-img": "^1.0.4",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "concurrently": "^3.6.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "crawler": "^1.1.4",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "express-graphql": "^0.6.12",
    "express-handlebars": "^3.0.0",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.0",
    "figures": "^2.0.0",
    "gm": "^1.23.1",
    "graphicsmagick": "0.0.1",
    "graphql": "^0.13.2",
    "imagemagick": "^0.1.3",
    "jimp": "^0.5.3",
